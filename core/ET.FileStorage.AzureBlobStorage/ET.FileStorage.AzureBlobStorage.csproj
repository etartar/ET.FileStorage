<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RepositoryUrl>https://github.com/etartar/ET.FileStorage</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Description>Azure Blob Storage dosya yükleme işlemleri için geliştirilmiş olan kütüphanedir.</Description>
    <Authors>Emir TARTAR</Authors>
    <Title>ET.FileStorage.AzureBlobStorage</Title>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.14.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ET.FileStorage.Core\ET.FileStorage.Core.csproj" />
  </ItemGroup>

</Project>
